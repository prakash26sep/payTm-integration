<html>

<head>
	<title>Merchant Check Out Page</title>
</head>

<body>
	<center>
		<h1>Please do not refresh this page...</h1>
	</center>

	<!-- After generating checksum, sending data to  paytm gateway-->
	<form method="post" action="https://securegw-stage.paytm.in/theia/processTransaction" name="f1">
		<table border="1">
			<tbody>
				<% for (name in restdata) { 
				console.log(name); 
				console.log(restdata[name] ); 
			%>
				<input type="hidden" name='<%= name %>' value='<%= restdata[name] %>'>

				<%}%>
			</tbody>
		</table>
		<script type="text/javascript">
			document.f1.submit();
		</script>
	</form>
</body>
</html>